{
  "name": "vasuzex",
  "version": "1.0.0",
  "description": "Laravel-inspired framework for Node.js monorepos",
  "type": "module",
  "packageManager": "pnpm@10.22.0",
  "imports": {
    "#framework": "./framework/index.js",
    "#framework/*": "./framework/*",
    "#database": "./database/index.js",
    "#database/*": "./database/*",
    "#models": "./database/models/index.js",
    "#models/*": "./database/models/*",
    "#config": "./config/index.cjs",
    "#config/*": "./config/*"
  },
  "bin": {
    "framework": "./framework/Console/cli.js"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "framework": "node framework/Console/cli.js",
    "dev": "dotenv -e .env -- turbo run dev --parallel",
    "dev:media": "cd apps/media-server && pnpm dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "start:blog": "turbo run start --filter=blog-api",
    "start:todo": "turbo run start --filter=todo-api",
    "db:migrate": "cd database && npx guruorm db:migrate",
    "db:migrate:status": "cd database && npx guruorm db:migrate:status",
    "db:seed": "cd database && npx guruorm db:seed",
    "db:reset": "pnpm db:migrate && pnpm db:seed",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint -- --fix",
    "clean": "turbo run clean && rm -rf node_modules",
    "clean:cache": "turbo run clean && rm -rf .turbo node_modules/.cache",
    "clean:all": "pnpm clean:cache && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && find . -name 'dist' -type d -prune -exec rm -rf '{}' +",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "preinstall": "npx only-allow pnpm",
    "dev:blog-api": "turbo run dev --filter=blog-api",
    "start:blog-api": "turbo run start --filter=blog-api",
    "dev:media-server": "turbo run dev --filter=@vasuzex/media-server",
    "start:media-server": "turbo run start --filter=@vasuzex/media-server",
    "dev:blog-api-api": "turbo run dev --filter=blog-api-api",
    "start:blog-api-api": "turbo run start --filter=blog-api-api",
    "dev:blog-api-web": "turbo run dev --filter=blog-api-web",
    "start:blog-api-web": "turbo run start --filter=blog-api-web"
  },
  "keywords": [
    "framework",
    "laravel",
    "monorepo",
    "nodejs"
  ],
  "author": "Vasuzex Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.0.0",
    "turbo": "^2.6.1"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "guruorm": "^2.0.0",
    "helmet": "^8.1.0",
    "maxmind": "^5.0.1",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "sharp": "^0.33.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rishicool/vasuzex.git"
  },
  "bugs": {
    "url": "https://github.com/rishicool/vasuzex/issues"
  },
  "homepage": "https://github.com/rishicool/vasuzex#readme",
  "publishConfig": {
    "access": "public"
  }
}
