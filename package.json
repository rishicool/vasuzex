{
  "name": "vasuzex",
  "version": "1.0.1",
  "description": "Laravel-inspired framework for Node.js monorepos",
  "type": "module",
  "main": "./framework/index.js",
  "exports": {
    ".": "./framework/index.js",
    "./Foundation": "./framework/Foundation/index.js",
    "./Foundation/*": "./framework/Foundation/*.js",
    "./Database": "./framework/Database/index.js",
    "./Database/*": "./framework/Database/*.js",
    "./Http": "./framework/Http/index.js",
    "./Http/*": "./framework/Http/*.js",
    "./Support": "./framework/Support/index.js",
    "./Support/*": "./framework/Support/*.js",
    "./Support/Facades": "./framework/Support/Facades/index.js",
    "./Services/*": "./framework/Services/*/index.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "create-vasuzex": "./bin/create-vasuzex.js",
    "vasuzex": "./framework/Console/cli.js"
  },
  "preferGlobal": true,
  "files": [
    "framework",
    "config",
    "database",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "imports": {
    "#framework": "./framework/index.js",
    "#framework/*": "./framework/*",
    "#database": "./database/index.js",
    "#database/*": "./database/*",
    "#models": "./database/models/index.js",
    "#models/*": "./database/models/*",
    "#config": "./config/index.cjs",
    "#config/*": "./config/*"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
  },
  "keywords": [
    "framework",
    "laravel",
    "monorepo",
    "nodejs"
  ],
  "author": "Vasuzex Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.0.0",
    "turbo": "^2.6.1"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "chalk": "^5.6.2",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.2.1",
    "fs-extra": "^11.3.2",
    "guruorm": "^2.0.0",
    "helmet": "^8.1.0",
    "inquirer": "^9.3.8",
    "maxmind": "^5.0.1",
    "multer": "^2.0.2",
    "ora": "^8.2.0",
    "pg": "^8.16.3",
    "sharp": "^0.33.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rishicool/vasuzex.git"
  },
  "bugs": {
    "url": "https://github.com/rishicool/vasuzex/issues"
  },
  "homepage": "https://github.com/rishicool/vasuzex#readme",
  "publishConfig": {
    "access": "public"
  }
}
